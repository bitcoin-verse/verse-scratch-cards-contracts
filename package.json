{
    "name": "prizegen-contracts",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "thirdweb-deploy": "npx thirdweb deploy",
        "foundry-size": "forge build --sizes",
        "foundry-test-log": "forge test -vv",
        "foundry-test-default": "forge test --gas-report -vvvv",
        "foundry-test-reports": "forge test --gas-report > gasreport.ansi -vvvv",
        "foundry-test-mainnet": "forge test --match-path contracts/tests/Mainnet/*.sol --gas-report -vvv",
        "foundry-test-mainnet-fork": "forge test --match-path contracts/tests/Mainnet/*.sol --rpc-url mainnet -vvv",
        "foundry-test-discount-hook": "forge test --match-contract DiscountHook_V3_Mainnet_Test --rpc-url mainnet -vvv",
        "foundry-test-swap-comparison": "forge test --match-test testRealHookFeeCalculation --rpc-url mainnet -vvv",
        "foundry-test-router-debug": "forge test --match-test testRouterDebugging --rpc-url mainnet -vvv",
        "foundry-test-swap-liquidity": "forge test --match-test testSwapWithLiquidity --rpc-url mainnet -vvv",
        "foundry-test-exact-swap": "forge test --match-test testExactSwapLikeYourTransaction --rpc-url mainnet -vvv",
        "foundry-test-mock-hook": "forge test --match-test testHookWithMockVaultCall --rpc-url mainnet -vvv",
        "foundry-test-actual-swaps": "forge test --match-test testActualSwapComparison --rpc-url mainnet -vvv",
        "foundry-test-verse-balance-comparison": "forge test --match-test testActualVerseBalanceComparison --rpc-url mainnet -vvv",
        "foundry-test-live-discount-hook": "forge test --match-contract DiscountHook_V3_Live_Test --rpc-url mainnet -vvv",
        "foundry-test-vebal-hook": "forge test --match-contract VeBALFeeDiscountHook_Live_Test --rpc-url mainnet -vvv",
        "foundry-test-polygon": "forge test --match-path contracts/tests/Polygon/*.sol --gas-report -vvv",
        "foundry-test-router-v3": "forge test --match-path contracts/tests/Polygon/TicketRouterV3.t.sol --gas-report -vvv",
        "foundry-test-router-v3-no-mock": "forge test --match-path contracts/tests/Polygon/TicketRouterV3NoMock.t.sol --gas-report -vvv",
        "foundry-test-flexible-draw": "forge test --match-path contracts/tests/Polygon/FlexibleDrawVRF.t.sol --gas-report -vvv",
        "foundry-test-spin-wheel": "forge test --match-path contracts/tests/SpinWheelVRF.t.sol --gas-report -vvv",
        "foundry-coverage": "forge coverage --report lcov && genhtml lcov.info --branch-coverage --output-dir coverage",
        "deploy-spin-wheel-dry": "forge script contracts/scripts/DeploySpinWheelPolygon.s.sol:DeploySpinWheelPolygon --chain-id=137 --rpc-url polygon",
        "add-vrf-consumer-dry": "forge script contracts/scripts/AddVRFConsumer.s.sol:AddVRFConsumer --chain-id=137 --rpc-url polygon",
        "remove-vrf-consumer-dry": "forge script contracts/scripts/RemoveVRFConsumer.s.sol:RemoveVRFConsumer --chain-id=137 --rpc-url polygon",
        "deploy-reel-vrf": "forge script contracts/scripts/Deployer.s.sol:DeployReelVRF --with-gas-price 270000000000 --slow --broadcast --verify --chain-id=137 --rpc-url polygon",
        "deploy-scratch-vrf": "forge script contracts/scripts/Deployer.s.sol:DeployScratchVRF --with-gas-price 99000000000 --slow --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
        "deploy-flexible-draw-vrf": "forge script contracts/scripts/DeployFlexibleDrawVRF.s.sol:DeployFlexibleDrawVRF --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
        "deploy-scratch-router": "forge script contracts/scripts/Deployer.s.sol:DeployTicketRouter --with-gas-price 99000000000 --slow --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
        "deploy-scratch-router-v3": "forge script contracts/scripts/Deployer.s.sol:DeployTicketRouterV3 --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
        "deploy-basket-swap": "forge script contracts/scripts/DeployBasketSwap.s.sol:DeployBasketSwap --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
        "deploy-spin-wheel": "forge script contracts/scripts/DeploySpinWheelPolygon.s.sol:DeploySpinWheelPolygon --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
        "add-vrf-consumer": "forge script contracts/scripts/AddVRFConsumer.s.sol:AddVRFConsumer --broadcast --legacy --chain-id=137 --rpc-url polygon",
        "remove-vrf-consumer": "forge script contracts/scripts/RemoveVRFConsumer.s.sol:RemoveVRFConsumer --broadcast --legacy --chain-id=137 --rpc-url polygon",
        "deploy-discounts-v3": "forge script contracts/scripts/DeployDiscounts_V3.s.sol:DeployDiscounts_V3 --broadcast --legacy --verify --chain-id=1 --rpc-url mainnet",
        "deploy-vebal-hook-dry": "forge script contracts/scripts/DeploySwapDiscountHook.s.sol:DeploySwapDiscountHook --chain-id=1 --rpc-url mainnet",
        "deploy-vebal-hook": "forge script contracts/scripts/DeploySwapDiscountHook.s.sol:DeploySwapDiscountHook --broadcast --legacy --verify --chain-id=1 --rpc-url mainnet",
        "deploy-permissive-hook-dry": "forge script contracts/scripts/DeployPermissiveHook.s.sol:DeployPermissiveHook --chain-id=1 --rpc-url mainnet",
        "deploy-permissive-hook": "forge script contracts/scripts/DeployPermissiveHook.s.sol:DeployPermissiveHook --broadcast --legacy --verify --chain-id=1 --rpc-url mainnet",
        "verify-reel-vrf": "forge verify-contract --chain-id=137 0x911D1e107a9599c491dDbDFA9736D11F85cE168e ReelVRF --watch"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@ethersproject/address": "^5.7.0",
        "hardhat": "^2.16.0",
        "hardhat-gas-reporter": "^1.0.9"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-foundry": "^1.0.3",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
        "@nomiclabs/hardhat-ethers": "^2.1.1",
        "@nomiclabs/hardhat-waffle": "^2.0.3",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "chai": "^4.3.6",
        "custom-error-test-helper": "^1.0.6",
        "ethers": "^5.7.1"
    }
}
